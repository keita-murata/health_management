<h1><%= @user.username %>のマイページ</h1>
<p>体温</p>
<p>運動メニュー</p>


<% if @user.id == current_user.id %>
  <%= @user.email %>
  <%= link_to "編集", edit_user_path(@user) %>
  <h3><%= @user.username %>さんが考案したトレーニングメニューリスト</h3>
  <% @user.trainings.each do |n| %>
      <%= n.title %>
      <%= n.content %>
  <% end %>
<% end %>